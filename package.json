{
  "name": "1panel-app-workshop",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@9.11.0",
  "description": "One-stop build for 1Panel local applications",
  "author": "Corentin Th <corentin.thomasset74+it-tools@gmail.com> (https://corentin.tech)",
  "license": "GNU GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/pooneyy/1panel-app-workshop"
  },
  "keywords": [
    "productivity",
    "converter",
    "website",
    "vuejs",
    "tools",
    "frontend",
    "tool",
    "developer-tools",
    "developer-productivity"
  ],
  "scripts": {
    "build": "vite build",
    "build:gh-pages": "vite build --base=/1panel-app-workshop/",
    "build:package": "vite build && pnpm package && pnpm clean",
    "build:package:gh-pages": "vite build --base=/1panel-app-workshop/ && pnpm package && pnpm clean",
    "build:preview": "pnpm clean && pnpm build && pnpm preview",
    "clean": "rimraf dist",
    "dev": "vite",
    "dev:gh-pages": "vite --base=/1panel-app-workshop/",
    "package": "tar -czf publish.tgz -C ./dist .",
    "package2zip": "tar --format=zip -cf publish.tar.zip -C ./dist .",
    "preview": "vite preview --port 5050",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "winrar": "winrar a -r -ep1 publish.zip ./dist/*"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.2",
    "@tabler/icons-vue": "^3.20.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vueuse/core": "^10.11.1",
    "@vueuse/head": "^2.0.0",
    "@vueuse/router": "^10.0.0",
    "date-fns": "^4.0.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "flexsearch": "^0.8.205",
    "highlight.js": "^11.11.1",
    "js-base64": "^3.7.7",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "marked": "^10.0.0",
    "mime-types": "^2.1.35",
    "monaco-editor": "^0.44.0",
    "naive-ui": "^2.35.0",
    "pinia": "^2.0.34",
    "vue": "^3.3.4",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.1.6",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.50",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "less": "^4.2.2",
    "rimraf": "^6.1.2",
    "typescript": "~5.2.2",
    "unocss": "^0.57.1",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.4.9",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.1"
  }
}
